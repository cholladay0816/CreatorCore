{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0|^8.1",
        "authorizenet/authorizenet": "^2.0",
        "aws/aws-sdk-php": "^3.178",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "google/recaptcha": "^1.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/cashier": "^12.6",
        "laravel/framework": "^8.40",
        "laravel/jetstream": "^2.3.0, !=2.3.1",
        "laravel/nova": "~4.0",
        "laravel/sail": "^1.0.1",
        "laravel/sanctum": "^2.6",
        "laravel/socialite": "^5.1",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.0",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpseclib/phpseclib": "^3.0.7",
        "stevebauman/location": "^6.3",
        "stripe/stripe-php": "^7.76"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3",
        "friendsofphp/php-cs-fixer": "^v3.4.0",
        "brianium/paratest": "^6.1",
        "laracasts/cypress": "^1.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
	"repositories": [
		{
			"type": "composer",
			"url": "https://nova.laravel.com"
		}
	],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "phpunit" : "phpunit --verbose --testdox",
        "phpcs" : "php-cs-fixer fix .",
        "phpcs_validate" : "php-cs-fixer fix . --dry-run --diff",

        "test": [
            "@phpunit",
            "@phpcs"
        ]
    }
}
